/********************************************
	see static void PeepHole(BBlock bb)
			in simp.c
 ********************************************/
#include <stdio.h>

int main(){
	int a = 26,b = 100;	
	a = a > 20 ? (a > 30? b+35: b+25): (b+15);
	printf("a = %d b = %d \n",a,b);
	return 0;
}

#if 0
#include <stdio.h>

int a = 1, b= 2;
int f(){
	printf("f()\n");
	return 1;
}
int g(){
	printf("g()\n");
	return 1;
}
int h(){
	printf("h()\n");
	return 1;
}
int main(){

	a > 0 ? f(): b > 0 ? g():h();
	printf("------------------\n");
	(a > 0 ? f(): b > 0) ? g():h();
	return 0;
